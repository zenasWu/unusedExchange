{{#list}}
<li class="p-item">
    <div class="p-img-con">
        <a href="./detail.html?productId={{id}}" class="link" target="_blank">
            <img src="{{imageHost}}{{mainImage}}" class="p-img" alt="{{name}}">
        </a>
    </div>
    <div class="p-name-con">
        <a href="./detail.html?productId={{id}}" class="link p-name" target="_blank">{{name}}</a>
    </div>
</li>
{{/list}}
{{^list}}
<p class="err-tip">抱歉,找不到相关的商品!!!</p>
{{/list}}
